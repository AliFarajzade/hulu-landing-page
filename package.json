{
  "name": "hulu-landing-page",
  "version": "1.0.0",
  "description": "Hulu Landing Page Clone built by Ali Farajzade",
  "default": "index.html",
  "browserslist": "last 10 versions",
  "scripts": {
    "sass:watch": "node-sass ./app/scss/main.scss ./app/css/style.css -w",
