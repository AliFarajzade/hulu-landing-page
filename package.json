  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "autoprefixer": "^10.4.0",
    "parcel": "^2.0.1",
    "postcss": "^8.4.4",
    "postcss-preset-env": "^7.0.1"
  }
}
